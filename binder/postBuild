#!/bin/bash
cpanm --local-lib=~/perl5 local::lib && eval $(perl -I ~/perl5/lib/perl5/ -Mlocal::lib)
cpanm --force IO::Async

cpanm  Module::Install
cpanm Module::Install::AuthorTests
cpanm Module::Install::CheckLib
cpanm Module::Install::XSUtil
cpanm LWP
lwp-request https://raw.githubusercontent.com/zmughal-p5CPAN/p5-Alt-Alien-ZMQ-Alien-ZMQ-latest/master/maint/install-zmq-libzmq.pl | perl - --notest Alt::Alien::ZMQ::Alien::ZMQ::latest ZMQ::LibZMQ3 Net::Async::ZMQ
#cpanm ZMQ::Constants
#cpanm ZMQ::LibZMQ3
cpanm Markdent@0.26 Markdown::Pod@0.00
#cpanm Devel::IPerl

cat /home/jovyan/.cpanm/work/**/build.log | tail -1000
exit 1
